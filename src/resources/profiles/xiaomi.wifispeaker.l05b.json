{
  "model": "xiaomi.wifispeaker.l05b",
  "name": "Xiaoai Speaker Play Enhanced",
  "services": [
    {
      "siid": 2,
      "type": "urn:miot-spec-v2:service:speaker:0000781C:xiaomi-l05b:1",
      "description": "Speaker",
      "properties": [
        {
          "piid": 1,
          "type": "urn:miot-spec-v2:property:volume:00000013:xiaomi-l05b:1",
          "name": "volume",
          "description": "Volume",
          "format": "uint8",
          "access": ["read", "write", "notify"],
          "value-range": [0, 100, 1]
        },
        {
          "piid": 2,
          "type": "urn:miot-spec-v2:property:mute:00000040:xiaomi-l05b:1",
          "name": "mute",
          "description": "Mute",
          "format": "bool",
          "access": ["read", "write", "notify"]
        }
      ]
    },
    {
      "siid": 3,
      "type": "urn:miot-spec-v2:service:play-control:0000781D:xiaomi-l05b:1",
      "description": "Play Control",
      "properties": [
        {
          "piid": 1,
          "type": "urn:miot-spec-v2:property:playing-state:00000041:xiaomi-l05b:1",
          "name": "playing-state",
          "description": "Playing State",
          "format": "uint8",
          "access": ["read", "notify"],
          "value-list": [
            {"value": 0, "description": "Paused"},
            {"value": 1, "description": "Playing"},
            {"value": 2, "description": "Buffering"}
          ]
        }
      ]
    },
    {
      "siid": 4,
      "type": "urn:miot-spec-v2:service:microphone:0000781E:xiaomi-l05b:1",
      "description": "Microphone",
      "properties": [
        {
          "piid": 1,
          "type": "urn:miot-spec-v2:property:mute:00000040:xiaomi-l05b:1",
          "name": "mic-mute",
          "description": "Mute",
          "format": "bool",
          "access": ["read", "write", "notify"]
        }
      ]
    }
  ],
  "ui_config": {
    "dashboard": {
      "overview_properties": [],
      "chart_properties": {}
    },
    "details": {
      "display_order": ["playing-state", "volume", "mute", "mic-mute"],
      "friendly_names": {
        "playing-state": "播放状态",
        "volume": "音量",
        "mute": "静音",
        "mic-mute": "麦克风静音"
      }
    }
  }
}
